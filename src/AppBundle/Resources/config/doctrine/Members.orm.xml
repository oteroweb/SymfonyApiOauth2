<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Members" table="members">
    <unique-constraints>
      <unique-constraint name="email" columns="email"/>
    </unique-constraints>
    <id name="memberId" type="integer" column="member_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateOfBirth" type="date" column="date_of_birth" nullable="true"/>
  </entity>
</doctrine-mapping>
